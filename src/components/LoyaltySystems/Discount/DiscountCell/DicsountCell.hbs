<div class="discount-cell">
    <form class="discount-cell__form">

    <div class="discount-cell__form__discount">
        <input id="discount-{{index}}-{{price}}-{{discount}}" class="discount-cell__form__discount_input" type="text" placeholder="%" value={{discount}}>
    </div>

    <div class="discount-cell__form__type">


    </div>
    <div class="discount-cell__form__price">
        <input id="price-{{index}}-{{price}}-{{discount}}" class="discount-cell__form__price_input" type="text" placeholder="1000" value={{price}}>
    </div>

    </form>

</div>
