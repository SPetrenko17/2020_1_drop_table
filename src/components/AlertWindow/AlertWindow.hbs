<div class="apple-card-published-qr-container">
    <div class="apple-card-published-qr-container__QR-window">
        <h4 class="apple-card-published-qr-container__QR-window_h4" >{{text}}</h4>
        {{#if cardLink}}
        <a class="apple-card-published-qr-container__QR-window_a" href="{{cardLink}}">{{cardLink}}</a>
        {{/if}}
        <img class="apple-card-published-qr-container__QR-window_img" src="{{qrImage}}">
    </div>
</div>
