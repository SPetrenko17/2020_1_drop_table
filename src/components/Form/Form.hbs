{{#each formFields}}
    <div class="input-field">
        <{{areaType}} class="input-field_input" {{inputOption}} type={{type}} id={{id}} value="{{data}}"{{readOnly}}>{{#if areaData }}{{areaData}}{{/if}}{{#if areaType }}</{{areaType}}>{{/if}}
        <label class="input-field_label" for={{id}}>{{labelData}}</label>
    </div>
{{/each}}
    {{#if redirect}}
        <div class="authorize__form-container__form__have-account">
            <label class="authorize__form-container__form__have-account__ha">{{redirect.textRedirect}} </label>
            <a href="{{redirect.link}}" class="authorize__form-container__form__have-account_a">{{redirect.linkText}}</a>
        </div>
    {{/if}}
<input ontouchstart="" class="input-field__submit" type="submit" value={{submitValue}}>
